<div class="modal fade" id="editMember" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button class="close" data-dismiss="modal" type="button">&times;</button>
        <h4 class="modal-title">Change Members</h4>
      </div>
      <div class="form-group">
        <div class="input-group" style="width: 100%;">

          <% _.each(members, function (member) { %>
            <div class="team team-wrapper" data-list-id="<%= member.id %>">
              <div class="card-list" id="card">
                <ul class="connectedSortable list-sortable"></ul>
                <a href="/member/show/<%= member.id %>">
                  <span>
                    <%= member.name %>
                  </span>
                </a>
              </div>
            </div>
          <% }) %>
          <% include new.ejs %>
          <div class="modal-footer">
            <button class="btn green" data-dismiss="modal">Cancel</button>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>
