<% _.each(cards, function (card) { %>
  <% if(card.owner == list.id ){ %>
    <div class="card open-card-details"
         data-card-id="<%= card.id %>"
         data-title="<%= card.header %>"
         data-toggle="modal"
         data-target="#card-details">
        <input type="hidden" name="owner" value="<%= card.owner %>">
        <div class="card-title" data-name-by-card-id="<%= card.id %>"><%= card.header %></div>
    </div>
    <% } %>
      <% }) %>
