<div class="modal fade" id="editListName" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button class="close" data-dismiss="modal" type="button">&times;</button>
        <h4 class="modal-title">Rename list</h4>
      </div>
      <div class="form-group">
        <div class="input-group" style="width: 100%;">
          <form action="/list/update/" id="rename-list" method="POST">

            <div class="modal-body clearfix">
              <input class="form-control" id="list-id" name="id" type="hidden">
              <input class="form-control" id="new-name" name="name" style="width: 100%;" type="text" >
              <input class="form-control" id="board-id" name="owner" type="hidden">
              <input name='_csrf' type='hidden' value='<%= _csrf %>'>
            </div>

            <div class="modal-footer">
              <button class="btn green" type="submit">Rename</button>
            </div>

          </form>
        </div>
      </div>
    </div>
  </div>
</div>
